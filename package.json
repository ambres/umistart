    "server:dev": "umi build & cross-env PORT=9096 NODE_ENV=development node --inspect ./server",
    "pub": "cross-env NODE_ENV=Publish && umi build && node ./server/autoupdate.main.js",
    "pub:dev": "cross-env UMI_ENV=local umi build && cross-env NODE_ENV=development  node ./server/autoupdate.main.js"
  },
