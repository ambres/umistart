{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=9095 umi dev",
    "build": "cross-env NODE_ENV=Publish && umi build",
    "lint": "eslint --ext .js src test",
    "precommit": "npm run lint",
    "prod": "npm run pub & cross-env PORT=9096 node ./server",
    "test": "jest",
    "server": "cross-env PORT=9096 node ./server",
